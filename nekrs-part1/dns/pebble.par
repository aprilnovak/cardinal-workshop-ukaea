[OCCA]
  backend = CPU

[MESH]
  file = ../pebble.re2

[GENERAL]
  polynomialOrder = 5
  numSteps = 2000
  dt = targetCFL=2.0 + initial=1e-4
  writeInterval = 500

[PRESSURE]
  residualTol = 1e-06

[VELOCITY]
  boundaryTypeMap = v, o, W, sym
  residualTol = 1e-07
  density = 0.1785
  viscosity = 2e-5

[TEMPERATURE]
  boundaryTypeMap = t, o, f, I
  residualTol = 1e-07
  rhoCp = 926.415
  conductivity = 0.1513

[CASEDATA]
  Re = 50
  D = 0.03
  inlet_T = 500
