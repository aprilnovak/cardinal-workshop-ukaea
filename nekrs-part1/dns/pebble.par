[OCCA]
  backend = CPU

[MESH]
  file = ../pebble.re2

[GENERAL]
  polynomialOrder = 5
  numSteps = 500
  dt = targetCFL=2.0 + initial=1e-4
  writeInterval = 100

[PRESSURE]
  residualTol = 1e-06

[VELOCITY]
  boundaryTypeMap = v, o, W, sym
  residualTol = 1e-07
  density = 1000.0
  viscosity = 1e-3

[CASEDATA]
  my_inlet_v = 0.001666
