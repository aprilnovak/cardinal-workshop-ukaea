[OCCA]
  backend = CPU

[MESH]
  file = ../../pebble.re2

[GENERAL]
  polynomialOrder = 3
  numSteps = 4000
  dt = targetCFL=2.0 + initial=1e-4
  writeInterval = 100

[PRESSURE]
  residualTol = 1e-06

[VELOCITY]
  boundaryTypeMap = v, o, W, sym
  residualTol = 1e-07
  density = 1000.0
  viscosity = 1e-3

[TEMPERATURE]
  boundaryTypeMap = t, o, f, I
  residualTol = 1e-07
  rhoCp = 4186000
  conductivity = 0.6

[CASEDATA]
  Re = 50
  D = 0.03
  inlet_T = 0
