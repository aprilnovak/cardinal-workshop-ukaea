[OCCA]
  backend = CPU

[MESH]
  file = ../pebble.re2

[GENERAL]
  startFrom = load.fld
  polynomialOrder = 10
  numSteps = 50000
  dt = 5e-4
  writeInterval = 25000

  regularization = hpfrt + nModes=2 + scalingCoeff=5

[PRESSURE]
  residualTol = 1e-06

[VELOCITY]
  boundaryTypeMap = v, o, W, sym
  residualTol = 1e-07
  density = 1.0
  viscosity = -2000

[TEMPERATURE]
  boundaryTypeMap = t, o, f, I
  residualTol = 1e-07
  rhoCp = 1.0
  conductivity = -13940
